<% for (var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].quantity %></td>
        <td>
            <input type="button" onclick="location.href='/update-user?id=<%= users[i]._id%>';" value="Edit" />        </td>
</tr>
<% } %>
<script src="/js/index.js"></script>